
<div class="posts">
 {{#each posts}}
<div class="post_card">
<div class="user-info">
    <img class="userPic" src= "{{this.profileURL}}"/>
<h4> <a href="#">
{{this.creator}}
</a>
    </h4>
</div>
<hr>
    
   
<div class="post_date">
   
   <p>

   {{this.createdAt}}
